      "bundleIdentifier": "com.thehappylions.pomotodo",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.745212572291-fgpgde952g0n32aknrluav8tb4f42bve"
        }
      }
    },
    "android": {
        "package": "com.thehappylions.pomotodo",
